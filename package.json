{
  "name": "react-code-input",
  "version": "1.0.9",
  "description": "React component for entering and validating numbers, text or password.",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack && webpack-dev-server --hot --content-base example/",
    "build": "webpack && cd styles && sass style.scss style.css",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=text ./node_modules/.bin/_mocha test/*.js && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage",
    "test_": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- -R spec --compilers js:babel-register && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/40818419/react-code-input.git"
  },
  "keywords": [
    "react",
    "react-component",
    "react-code-input",
    "code",
    "input",
    "chiffre",
    "field"
  ],
  "author": "Konstantin Kulinicenko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/40818419/react-code-input/issues"
  },
  "homepage": "https://github.com/40818419/react-code-input#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "react-dom": "^15.4.2",
    "react": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-istanbul": "^3.1.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "codacy-coverage": "^2.0.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.7.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^9.9.1",
    "jsdom-global": "2.1.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "node-sass": "^3.11.3",
    "nyc": "^10.0.0",
    "postcss-loader": "^1.1.1",
    "react-addons-test-utils": "^15.4.1",
    "react-markdown": "^2.4.2",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
